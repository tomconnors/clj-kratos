  org.clj-commons/digest {:mvn/version "1.4.100"}
  com.fasterxml.uuid/java-uuid-generator {:mvn/version "5.1.0"}}
 :paths ["src" "resources"]
 :aliases
 {:dev {:extra-deps {djblue/portal {:mvn/version "0.61.0"}
                     com.aayushatharva.brotli4j/native-osx-x86_64    {:mvn/version "1.20.0"}
                     com.aayushatharva.brotli4j/native-osx-aarch64   {:mvn/version "1.20.0"}
                     com.aayushatharva.brotli4j/native-linux-aarch64 {:mvn/version "1.20.0"}
                     io.github.tonsky/clj-reload {:mvn/version "1.0.0"}}
        :jvm-opts ["-Dclojure.core.async.go-checking=true"]}
  :cider {:extra-deps {cider/cider-nrepl {:mvn/version "0.58.0"}
                       nrepl/nrepl {:mvn/version "1.5.1"}}
          :main-opts ["-m" "nrepl.cmdline" "--middleware" "[cider.nrepl/cider-middleware]"]}
  :build {:deps {org.clojure/clojure {:mvn/version "1.12.3"}
                 io.github.clojure/tools.build {:git/sha "20de3036218268802311f96fa6cfa9123c2963f1"}
                 com.thheller/shadow-css {:mvn/version "0.6.2"}
                 com.nextjournal/beholder {:mvn/version "1.0.3"}}
          :ns-default build}}}
