{:deps
 {org.clojure/clojure {:mvn/version "1.12.3"}
  org.clojure/core.async {:mvn/version "1.8.741"}
  cheshire/cheshire {:mvn/version "6.1.0"} ;; json parsing
  ring/ring {:mvn/version "1.15.3"}
  ring/ring-defaults {:mvn/version "0.7.0"}
  ring/ring-json {:mvn/version "0.5.1"}
  com.datomic/peer {:mvn/version "1.0.7469"}
  metosin/reitit {:mvn/version "0.9.1"} ;; routing
  http-kit/http-kit {:mvn/version "2.9.0-beta2"}
  mount/mount {:mvn/version "0.1.23"}
  metosin/malli {:mvn/version "0.19.2"} ;; data schema specification/validation
  dev.weavejester/medley {:mvn/version "1.9.0"} ;; Utility code
  fmnoise/flow {:mvn/version "4.3.0"} ;; Flow control for complicated conditionals
  com.thheller/shadow-css {:mvn/version "0.6.2"} ;; CSS generation
  ;; fast hiccup:
  dev.onionpancakes/chassis    {:mvn/version "1.0.365"}
  ;; fast json:
  com.cnuernber/charred        {:mvn/version "1.037"}
  dev.data-star.clojure/sdk {:mvn/version "1.0.0-RC3"}
  dev.data-star.clojure/http-kit {:mvn/version "1.0.0-RC3"}
